# .env.example
# Copy this file to .env and fill in your actual credentials.
# DO NOT commit your .env file to version control.

# Discord Bot Token (Required)
DISCORD_BOT_TOKEN=YOUR_DISCORD_BOT_TOKEN_HERE


# Optional: Override specific LLM provider keys/URLs if needed
# Example for separate OpenAI keys/base URLs (if not using OpenRouter for them)
OPENAI_PRIMARY_API_KEY=YOUR_PRIMARY_LLM_API_KEY
OPENAI_PRIMARY_BASE_URL=https://api.openai.com/v1
OPENAI_SECONDARY_API_KEY=YOUR_SECONDARY_LLM_API_KEY
OPENAI_SECONDARY_BASE_URL=https://api.openai.com/v1

# Optional: Override configuration file paths (absolute paths recommended)
CONFIG_FILE_PATH=/path/to/your/config.yaml
SCORING_RULES_FILE_PATH=/path/to/your/scoring_rules.json
PERSONA_PROMPT_FILE_PATH=/path/to/your/prompts.yaml
CONTEXT_STORAGE_PATH=/path/to/your/data/directory

# Optional: Override log level (trace, debug, info, warn, error)
# LOG_LEVEL=debug
